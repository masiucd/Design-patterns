    "outDir": "./dist/",
    "noImplicitAny": true,
    "moduleResolution": "classic",
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "esModuleInterop": true,
    "allowJs": true
  }
}
